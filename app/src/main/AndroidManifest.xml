<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wukun.databinding">

  <uses-permission android:name="android.permission.INTERNET"/>
  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name="com.wukun.databinding.view.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <!--基本用法-->
    <activity android:name="com.wukun.databinding.view.BasicActivity"/>
    <!--显示图片-->
    <activity android:name="com.wukun.databinding.view.ImageActivity"/>
    <!--更多用法-->
    <activity android:name="com.wukun.databinding.view.MoreMethodActivity"/>
    <!--点击事件-->
    <activity android:name="com.wukun.databinding.view.ClickActivity"/>
    <!--ListView-->
    <activity android:name="com.wukun.databinding.view.ListViewActivity"/>
    <!--数据更新-->
    <activity android:name="com.wukun.databinding.view.DataNotifyActivity"/>
  </application>

</manifest>